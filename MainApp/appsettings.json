{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Iframe": {
    "EmbeddedOrigin": "https://localhost:7129",
    "EmbeddedPath": "/embedded"
  },
  "Jwt": {
    "Issuer": "MainApp",
    "Audience": "EmbeddedPages",
    "PrivateKeyPemPath": "../Keys/mainapp-private.pem",
    "SecretClaimName": "SecretParam",
    "SecretClaimValue": "SecretValue",
    "TokenLifetimeMinutes": 5
  },
  "AllowedHosts": "*"
}
